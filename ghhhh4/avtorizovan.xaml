<Window x:Class="ghhhh4.avtorizovan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ghhhh4"
        mc:Ignorable="d"
        Title="avtorizovan" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="exit" Content="Выйти" Grid.Column="0" Grid.Row="0" Click="exit_Click"/>
        <Button x:Name="redactor_testa" Content="Редактор" Grid.Column="1" Grid.Row="0" Click="redactor_testa_Click"/>
        <Button x:Name="proti" Content="Пройти тест" Grid.Column="2" Grid.Row="0" Click="proti_Click"/>

        <Frame x:Name="izmen" NavigationUIVisibility="Hidden" Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="3" />
        <Frame x:Name="proxodtest" Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="3"/>

    </Grid>
</Window>
